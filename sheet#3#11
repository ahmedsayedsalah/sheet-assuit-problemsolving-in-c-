//code
//library contains all of functions that you may use
#include <bits/stdc++.h>
using namespace std;
//main function
int main()
{
    //fast input and output

    ios_base::sync_with_stdio(false);
    cin.tie(NULL);

    //start
    int num,sum=0;
    string strdigit;
    cin>>num>>strdigit;
    char arrOfChar [num];
    strcpy(arrOfChar,strdigit.c_str());
    for(int i=0;i<num;i++)
    {
       sum+=((int)(arrOfChar[i])-48);
    }
    cout<<sum;
   return 0;
}
