//code
//library contains all of functions that you may use
#include <bits/stdc++.h>
using namespace std;
//main function
int main()
{
    //fast input and output
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);

    int n;
    cin>>n;
    int list[n];
    for(int i=0;i<(sizeof(list)/sizeof(list[0]));i++)
    {
        cin>>list[i];
        if(list[i]<=10)
            cout<<"A["<<i<<"] = "<<list[i]<<endl;
    }
   return 0;
}
