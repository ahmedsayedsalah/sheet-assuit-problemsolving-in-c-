//code
#include <bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    cin>>n;
    bool a=true;
   for(int i=2;i<=sqrt(n);i++) //or condition (n/2)
   {
       if(n%i==0)
          a=false;
   }
   if(a==true)
    cout<<"YES";
   else
    cout<<"NO";
    return 0;
}
