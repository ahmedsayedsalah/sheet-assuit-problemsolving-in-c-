//code
//library contains all of functions that you may use
#include <bits/stdc++.h>
using namespace std;
//main function
int main()
{
    long long n;
    cin>>n;
    long long row=n/4,column=0;
    if(row % 2 ==0)
        column=n%4;
    else
        column=3-(n%4);
    cout<<row<<" "<<column;
   return 0;
}
