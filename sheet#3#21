//code
//library contains all of functions that you may use
#include <bits/stdc++.h>
using namespace std;
//main function
int main()
{
    //fast input and output
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);

    //start
    int n,m;
    cin>>n>>m;
    int arr[n];
    int counter[m+1]={0};
    for(int i=0;i<n;i++)
    {
        cin>>arr[i];
        counter[arr[i]]++;
    }
    for(int a=1;a<=m;a++)//1 2 3 4 5
          cout<<counter[a]<<endl;
   return 0;
}
