//code
//library contains all of functions that you may use
#include <bits/stdc++.h>
using namespace std;
//main function
int main()
{
    //fast input and output
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);

    //start
    int n,m;
    cin>>n>>m;
    int arr[n][m];
    for(auto& a:arr)
        for(auto& b:a)
           cin>>b;
    for(auto& a:arr)
    {
        reverse(a,a+(sizeof(a)/sizeof(int)));
        for(auto& b:a)
           cout<<b<<" ";
    cout<<endl;
    }
   return 0;
}
