//code
//library contains all of functions that you may use
#include <bits/stdc++.h>
using namespace std;
//main function
int main()
{
    //fast input and output
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);

    int n;
    cin>>n; 
    long long arr[n],x,c;
        for (int i = 0; i < n; i++) { 
            cin>>arr[i];
        }
        cin>>x; //0
        bool a=false;
        for (int i = 0; i < n; i++) { 
            c=arr[i];
            if(x==c) 
            {
                cout<<i;
                a=true;
                break;
            }
            else
            {
                a=false;
            }
        }
        if(a==false)
            cout<<-1;
   return 0;
}
