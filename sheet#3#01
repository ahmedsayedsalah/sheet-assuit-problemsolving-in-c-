//code
//library contains all of functions that you may use
#include <bits/stdc++.h>
using namespace std;
//main function
int main()
{
    //fast input and output
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);

    int n;
    cin>>n;
    long long arr[n];
    long long sum=0;
        for (int i = 0; i < n; i++) {
            cin>>arr[i];
            sum+=arr[i];
        }
        cout<<abs(sum);
   return 0;
}
