#include <bits/stdc++.h>
using namespace std;
int main()
{
    while(true)
    {
        int pass;
        cin>>pass;
        if(pass==1999)
        {
            cout<<"Correct"; 
            break; // loop بتكسر ال
        }
        else
            cout<<"Wrong"<<endl;
    }
    return 0;
}
