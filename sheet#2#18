//code
//library contains all of functions that you may use
#include <bits/stdc++.h>
using namespace std;
//main function
int main()
{
    while(true)
    {
        int n,m,sum=0;
        cin>>n>>m;
        if(n<=0 || m<=0) // to prevent numbers that is less than or equal zero.
            break;
        for(int i=min(n,m);i<=max(n,m);i++) //loop for numbers from n to m and Vice Versa. 
        {
            cout<<i<<" ";
            sum+=i;
        }
        cout<<"sum ="<<sum<<endl;
    }
   return 0;
}
