//code
//library contains all of functions that you may use
#include <bits/stdc++.h>
using namespace std;
//main function
int main()
{
    int n;
    char character;
    cin>>character>>n;
    while(n--)
    {
        int num;
        cin>>num;
        for(int i=1;i<=num;i++)
            cout<<character;
        cout<<endl;
    }
   return 0;
}
